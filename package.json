{
  "name": "cloud-player",
  "version": "5.5.0",
  "description": "Music/video player to play music from YouTube, SoundCloud and Mixcloud",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "release": "ng build --prod && npm run sw && cp CNAME ./dist",
    "lint": "ng lint",
    "sw": "sw-precache --root=dist --config=swconfig.js"
  },
  "keywords": [
    "angular",
    "angular6",
    "typescript",
    "soundcloud",
    "youtube",
    "cloudplayer",
    "backbone",
    "music",
    "audio",
    "video",
    "player"
  ],
  "contributors": [
    "Cloud-Player Development Team <hello@cloud-player.io> (https://github.com/Cloud-Player)"
  ],
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloud-player/web.git"
  },
  "bug": {
    "url": "https://github.com/cloud-player/web/issues"
  },
  "dependencies": {
    "@angular/animations": "7.1.1",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
